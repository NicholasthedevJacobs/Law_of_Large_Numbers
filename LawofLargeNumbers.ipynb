    "import numpy as np\n",
    "from numpy.random import randn\n",
    "\n",
    "counter = 0\n",
    "sample_size = 100\n",
    "\n",
    "for i in randn(sample_size):\n",
    "    if i > -1 and i < 1:\n",
    "        counter = counter + 1\n",
    "\n",
    "answer = counter / sample_size\n",
    "\n",
    "print(answer)\n",
    "        \n"
   ]
